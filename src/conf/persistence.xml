<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="AlphaDirectKPIManagementPU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>com.alpha.data.entities.ClaimAsOnDate</class>
    <class>com.alpha.data.entities.PercentageCostPerPulaGwp</class>
    <class>com.alpha.data.entities.DaysToIssueQuotesUnderwriting</class>
    <class>com.alpha.data.entities.PercentageComplianceUnderwriting</class>
    <class>com.alpha.data.entities.PercentageClosedPolicyUnderwitting</class>
    <class>com.alpha.data.entities.Agent</class>
    <class>com.alpha.data.entities.ComplianceCashOutBd</class>
    <class>com.alpha.data.entities.MotorNonMotorRatio</class>
    <class>com.alpha.data.entities.RenewalRationBd</class>
    <class>com.alpha.data.entities.PrecentageComplianceBd</class>
    <class>com.alpha.data.entities.TransactionReportByProduct</class>
    <class>com.alpha.data.entities.RdPartyRecoveries</class>
    <class>com.alpha.data.entities.AssessmentCostPerClaim</class>
    <class>com.alpha.data.entities.PremiumBordereauxReport</class>
    <class>com.alpha.data.entities.PercentageComplianceClaims</class>
    <exclude-unlisted-classes>false</exclude-unlisted-classes>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/YOUR_DB"/>
      <property name="javax.persistence.jdbc.user" value="YOUR_USER_NAME"/>
      <property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver"/>
      <property name="javax.persistence.jdbc.password" value="YOUR_PASSWORD"/>
    </properties>
  </persistence-unit>
</persistence>
